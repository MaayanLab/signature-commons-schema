{
  "$id": "/dcic/signature-commons-schema/v5/meta/entity/ilincs-gene.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Gene metadata as described by http://www.ilincs.org/ilincs.json",
  "allOf": [
    {
      "$ref": "/dcic/signature-commons-schema/v5/core/meta.json"
    },
    {
      "type": "object",
      "properties": {
        "chromosome": {
          "type": "string"
        },
        "dbxrefs": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "geneid": {
          "type": "number",
          "format": "double"
        },
        "homologeneid": {
          "type": "number",
          "format": "double"
        },
        "locustag": {
          "type": "string"
        },
        "maplocation": {
          "type": "string"
        },
        "namenomauth": {
          "type": "string"
        },
        "nomstatus": {
          "type": "string"
        },
        "symbol": {
          "type": "string"
        },
        "symbonomauth": {
          "type": "string"
        },
        "synonyms": {
          "type": "string"
        },
        "taxid": {
          "type": "number",
          "format": "double"
        },
        "typeofgene": {
          "type": "string"
        }
      },
      "required": [
        "geneid"
      ]
    }
  ]
}